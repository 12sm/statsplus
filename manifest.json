{
  "name": "StatsPlus for CoinStats",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Additional Stats for CoinStats Portfolios",
  "homepage_url": "https://codepen.io/michaelshoup/pen/GRWEQrM",
  "background": {
    "scripts": [
      "background.js"
